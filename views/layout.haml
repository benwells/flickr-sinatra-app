!!!
%html
  %header
    %script{src:"http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"}
    %script{src:"http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.2/jquery-ui.min.js"}
    %script{:src=>"../js/jquery.transform-0.6.2.min.js"}
    %script{:src=>"../js/jquery.animate-shadow-min.js"}

    %link{:rel=>"stylesheet", :href=>"//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.0.3/css/font-awesome.min.css"}

    -# New stuff added 05/23/14
    %link{:rel=>"stylesheet", :href=>"//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css"}
    %link{:rel=>"stylesheet", :href=>"../css/theme.css"}
    %link{:rel=>"stylesheet", :href=>"../css/slick.css"}
    %link{:rel=>"stylesheet", :href=>"../css/app.css"}

    -# Photo Desk crap.
    %link{:rel=>"stylesheet", :href=>"../css/photoDesk.css"}

  %body
    - if flash[:notice]
      %p.alert.alert-info= flash[:notice]
    =yield
  %script{:src=>"https://code.jquery.com/jquery-1.11.0.min.js"}


  -# New stuff added 05/23/14
  %script{:src=>"//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"}

  -# Some of the photo desk crap is depreciated so we need this to make it work.
  %script{:src=>"http://code.jquery.com/jquery-migrate-1.2.1.js"}


  %script{:src=>"../js/slick.min.js"}
  %script{:src=>"../js/app.js"}

  -# Photo Desk crap.
  %script{:src=>"../js/photoDesk.js"}
